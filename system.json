{
	"id": "fathomlessgears",
	"version": "#{VERSION}#",
	"title": "Fathomless Gears",
	"description": "A Foundry VTT system for Fathomless Gears, the mech fishing game by Ralf Ziegler",
	"background": "systems/fathomlessgears/assets/fg-logo-simple.png",
	"authors": [
		{
			"name": "Twelfth",
			"url": "https://github.com/twelfthrootoftwo"
		}
	],
	"esmodules": ["src/fathomlessgears.js", "scripts/jszip.js"],
	"styles": [
		"styles/theme.css",
		"styles/sheet.css",
		"styles/dialogs.css",
		"styles/utils.css",
		"styles/chat.css",
		"styles/canvas.css",
		"styles/grid.css"
	],
	"packs": [
		{
			"name": "core_macros",
			"label": "FG Macros",
			"system": "fathomlessgears",
			"path": "./packs/core_macros.db",
			"type": "Macro"
		},
		{
			"name": "grid_type",
			"label": "Grid Types",
			"system": "fathomlessgears",
			"path": "./packs/grid_types.db",
			"type": "Item"
		},
		{
			"name": "conditions",
			"label": "FG Conditions",
			"system": "fathomlessgears",
			"path": "./packs/conditions.db",
			"type": "Item"
		},
		{
			"name": "touch_of_the_deep",
			"label": "FG Touch of the Deep",
			"system": "fathomlessgears",
			"path": "./packs/touch_of_the_deep.db",
			"type": "Item"
		},
		{
			"name": "injuries",
			"label": "FG Injuries",
			"system": "fathomlessgears",
			"path": "./packs/injuries.db",
			"type": "Item"
		},
		{
			"name": "internal_tags",
			"label": "FG Tags",
			"system": "fathomlessgears",
			"path": "./packs/internal_tags.db",
			"type": "Item"
		},
		{
			"name": "fg_roll_tables",
			"label": "FG Rollable Tables",
			"system": "fathomlessgears",
			"path": "./packs/fg_roll_tables.db",
			"type": "RollTable"
		}
	],
	"languages": [
		{
			"lang": "en",
			"name": "English",
			"path": "lang/en.json"
		}
	],
	"compatibility": {
		"minimum": "11",
		"verified": "11.315"
	},
	"relationships": {
		"requires": [
			{
				"id": "token-action-hud-FG",
				"type": "module",
				"manifest": "https://github.com/twelfthrootoftwo/fvtt-token-action-hud-FG/releases/latest/download/module.json",
				"compatibility": [
					{
						"minimum": "0.3.0",
						"verified": "0.3.0"
					}
				]
			},
			{
				"id": "token-action-hud-core",
				"type": "module",
				"compatibility": [
					{
						"minimum": "1.5.0",
						"maximum": "1.5",
						"verified": "1.5.0"
					}
				]
			},
			{
				"id": "socketlib"
			},
			{
				"id": "statuscounter",
				"type": "module",
				"compatibility": [
					{
						"minimum": "2.1.1",
						"verified": "2.1.1"
					}
				]
			}
		]
	},
	"gridDistance": 1,
	"url": "#{URL}#",
	"manifest": "#{MANIFEST}#",
	"download": "#{DOWNLOAD}#",
	"persistentStorage": true
}
